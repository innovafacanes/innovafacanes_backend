{
  "kind": "singleType",
  "collectionName": "contactes",
  "info": {
    "singularName": "contacte",
    "pluralName": "contactes",
    "displayName": "Contacte"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "nom_empresa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "default": "INNOVA FAÇANES S.L."
    },
    "direccio_empresa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "default": "C/Praga 15, 08191, Rubí",
      "required": true
    },
    "telefon_empresa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "default": "+34 93 713 67 03",
      "required": true
    },
    "mobil_empresa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "default": "+34 62 659 60 01"
    },
    "email_empresa": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "default": "innovafacanes@innovafacanes.cat"
    }
  }
}
